{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.10",
    "node-fetch": "^2.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "socket.io-client": "^2.1.0"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-debug": "react-scripts --inspect-brk=0.0.0.0:9229 test --env=jsdom --runInBand -i",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "eslint": "node ./node_modules/eslint/bin/eslint.js  --ignore-path ./.eslintignore ./",
    "eslint-auto-fix": "node ./node_modules/eslint/bin/eslint.js --ignore-path ./.eslintignore ./ --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "jest-fetch-mock": "^1.5.0",
    "mock-local-storage": "^1.0.5",
    "sinon": "^5.0.3"
  }
}
