{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "lodash": "^4.17.10",
    "node-fetch": "^2.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "socket.io-client": "^2.1.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.14.0"
  },
  "scripts": {
    "build": "webpack --config ./prod.webpack.config.js && cp ./src/theme/index.html ../public/ && cp ./src/theme/assets ../public/",
    "build-dev": "webpack --config ./dev.webpack.config.js && cp ./src/theme/index.html ../public/ && cp ./src/theme/assets ../public/",
    "test": "jest --env=jsdom",
    "test-debug": "react-scripts --inspect-brk=0.0.0.0:9229 test --env=jsdom --runInBand -i",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "eslint": "node ./node_modules/eslint/bin/eslint.js  --ignore-path ./.eslintignore ./",
    "eslint-auto-fix": "node ./node_modules/eslint/bin/eslint.js --ignore-path ./.eslintignore ./ --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/registerServiceWorker.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "moduleNameMapper": { 
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js" 
    }
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "eslint-plugin-react": "^7.7.0",
    "jest-fetch-mock": "^1.5.0",
    "mock-local-storage": "^1.0.5",
    "react-router-test-context": "^0.1.0",
    "sinon": "^5.0.3",
    "webpack-cli": "^3.0.8"
  }
}
