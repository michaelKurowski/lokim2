{
  "name": "lokim",
  "version": "0.0.1",
  "description": "Ultra secure communicator server",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "test": "./node_modules/mocha/bin/mocha './.tests/**/*.spec.js'",
    "eslint": "node ./node_modules/eslint/bin/eslint.js --ignore-path ./.eslintignore ./",
    "eslint-auto-fix": "node ./node_modules/eslint/bin/eslint.js --ignore-path ./.eslintignore ./ --fix",
    "generate-docs": "node ./node_modules/jsdoc/jsdoc -r ./ws-routes/",
    "test-coverage": "node ./node_modules/nyc/bin/nyc --all --check-coverage npm test",
    "generate-config": "node -e 'require(\"./configFileService\")().generateConfig()'",
    "generate-client-files": "npm run --prefix frontEnd build && mv ./frontEnd/build ./public"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "out/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelKurowski/lokim2.git"
  },
  "keywords": [
    "lokim",
    "server"
  ],
  "author": "Jordan Moss, Michael Kurowski, Marcin Grzeszczak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/michaelKurowski/lokim2/issues"
  },
  "homepage": "https://github.com/michaelKurowski/lokim2#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.2",
    "express-mongo-sanitize": "^1.3.2",
    "express-session": "^1.15.6",
    "mongoose": "^5.0.8",
    "mongoose-unique-validator": "^2.0.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "socket.io": "^2.1.0",
    "uuid": "^3.2.1",
    "winston": "^2.4.0",
    "chai": "^4.1.2",
    "winston-transport": "^3.0.1"
  },
  "devDependencies": {
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-no-only-tests": "^2.0.0",
    "jsdoc": "^3.5.5",
    "mocha": "^5.0.4",
    "node-mocks-http": "^1.5.8",
    "nyc": "^11.7.1",
    "sinon": "^4.4.2",
    "socket.io-client": "^2.1.0"
  }
}
